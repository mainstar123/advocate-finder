{
  "name": "solace-candidate-assignment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:generate:schema": "drizzle-kit generate",
    "db:migrate:up": "ts-node -P tsconfig.cjs.json ./src/db/migrate.ts",
    "db:migrate:push": "drizzle-kit push",
    "db:seed": "ts-node -P tsconfig.cjs.json ./src/db/seed.ts",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run",
    "ci": "npm run lint && npm run format && npm run test && npm run e2e:run",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "antd": "^5.24.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "ioredis": "^5.6.0",
    "next": "^14.2.19",
    "pg": "^8.13.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chai": "^5.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.2.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.40",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
